{
  "name": "@vsf-enterprise/storefront-sapcc",
  "version": "2.0.0",
  "license": "VSFEL",
  "private": true,
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "dev": "concurrently \"nuxt\" \"yarn dev:middleware\"",
    "dev:middleware": "node middleware.js",
    "dev:frontend": "nuxt",
    "build": "nuxt build --modern=client",
    "build:analyze": "nuxt build -a --modern=client",
    "start": "nuxt start --modern=client",
    "start:middleware": "node middleware.js",
    "lhci:mobile": "lhci autorun",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "test": "jest",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --headed --browser chrome",
    "e2e:run:hl": "cypress run --browser chrome",
    "e2e:run:demo": "cypress run --headed --browser chrome --config baseUrl=https://demo-sapcc.europe-west1.gcp.storefrontcloud.io",
    "e2e:run:demo:hl": "cypress run --browser chrome --config baseUrl=https://demo-sapcc.europe-west1.gcp.storefrontcloud.io",
    "e2e:generate-report": "yarn -s mochawesome-merge \"__tests__/e2e/report/*.json\" > \"__tests__/e2e/report.json\" && yarn -s marge __tests__/e2e/report.json -o \"__tests__/e2e/report\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@nuxt/image": "^0.6.2",
    "@nuxtjs/composition-api": "0.31",
    "@nuxtjs/i18n": "^7.2.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "@pinia/nuxt": "0.1.9",
    "@storefront-ui/vue": "^0.13.6",
    "@vue-storefront/multistore": "^1.0.0",
    "@vsf-enterprise/sapcc": "2.0.0-c.0",
    "@vue-storefront/http-cache": "^2.7.5",
    "@vue-storefront/middleware": "^2.7.5",
    "@vue-storefront/nuxt": "^2.7.5",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^2.6.5",
    "node-cache": "^5.1.2",
    "nuxt": "^2.15.7",
    "patch-package": "^6.5.0",
    "pinia": "2.0.28",
    "postinstall-postinstall": "^2.1.0",
    "vee-validate": "^3.2.3",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-scrollto": "^2.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@lhci/cli": "^0.11.0",
    "@nuxt/types": "^0.7.9",
    "@nuxt/typescript-build": "^2.0.0",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "27.5.1",
    "concurrently": "^7.0.0",
    "cypress": "12.5.1",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "1.1.2",
    "eslint": "8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^7.0.1",
    "jest": "27.5.1",
    "lint-staged": "^11.1.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "vue-eslint-parser": "^7.10.0",
    "ts-jest": "27.1.4",
    "ts-loader": "^8",
    "vue-jest": "^4.0.0-0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "vue": "2.6.14",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "@pinia/nuxt": "0.1.9",
    "pinia": "2.0.28"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  }
}
